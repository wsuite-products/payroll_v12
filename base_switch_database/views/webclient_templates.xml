<odoo>

    <template id="assets_backend" name="base_switch_database assets"
              inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript"
                    src="/base_switch_database/static/src/js/base_switch_database.js"/>
        </xpath>
    </template>

    <template id="assets_frontend" name="base_switch_database frontendassets"
              inherit_id="web.assets_frontend">
        <xpath expr="." position="inside">
            <script type="text/javascript"
                    src="/base_switch_database/static/src/js/base_switch_database_frontend.js"/>
        </xpath>
    </template>


</odoo>
