<odoo>

    <record id="crm_lead_view_form_inherit_wadmin_campaign_inherit" model="ir.ui.view">
        <field name="name">crm.lead.form.inherit.wadmin.campaign.inherit</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm_lead_extended.crm_lead_view_form_inherit_wadmin_campaign"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='crm_lead_line_ids']/tree" position="inside">
                <button name="%(action_create_rfq_crm_lead)d"
                        id="create_rfq" type="action" string="Create RFQ"
                        class="oe_highlight"/>
            </xpath>
        </field>
    </record>

</odoo>
