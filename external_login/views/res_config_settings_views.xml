<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="res_config_settings_view_form_public_key" model="ir.ui.view">
		<field name="name">res.config.settings.view.form.inherit.public.key</field>
		<field name="model">res.config.settings</field>
		<field name="priority" eval="25"/>
		<field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
		<field name="arch" type="xml">
			<xpath expr="//div[@data-key='general_settings']" position="inside">
                <div id="public_key">
                    <h2>Public Key</h2>
                    <div class="row mt16 o_settings_container" id="general_public_key">
                        <div class="col-12 col-lg-6 o_setting_box" id="public_key">
                            <div class="o_setting_right_pane">
                                <field name="public_key"/>
                                <div class="text-muted">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
			</xpath>
		</field>
	</record>
</odoo>
